{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src": [
        "src"
      ],
      "assets": [
        "assets"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}